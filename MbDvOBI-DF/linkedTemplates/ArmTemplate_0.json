{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "MbDvOBI-DF"
		},
		"AzureBlobStorage_importstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_importstorage'"
		},
		"AzureDataLakeStorage_permanentstorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_permanentstorage'"
		},
		"linkedService1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedService1'"
		},
		"BlobStorage_importstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorage_importstorage'"
		},
		"AzureDataLakeStore_dgdvodltmads_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStore_dgdvodltmads'"
		},
		"AzureDataLakeStorage_permanentstorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dgdvodltmads.dfs.core.windows.net"
		},
		"AzureDataLakeStore_dgdvodltmads_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dgdvodltmads.dfs.core.windows.net"
		},
		"Datalake_LandingZone_Raw_parkingdata_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"Datalake_LandingZone_Raw_parkingdata_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "datalake/LandingZone/Raw/parking-data"
		},
		"Blob_mbdvobist_parkingdata_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"Blob_mbdvobist_parkingdata_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "parking-data"
		},
		"Datalake_LandingZone_Raw_parkingdata1_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"Datalake_LandingZone_Raw_parkingdata1_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "datalake/LandingZone/Raw/parking-data"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobStorage_importstorage')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('AzureBlobStorage_importstorage_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStorage_permanentstorage')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('AzureDataLakeStorage_permanentstorage_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('AzureDataLakeStorage_permanentstorage_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/linkedService1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('linkedService1_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/BlobStorage_importstorage')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('BlobStorage_importstorage_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStore_dgdvodltmads')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('AzureDataLakeStore_dgdvodltmads_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('AzureDataLakeStore_dgdvodltmads_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Datalake_LandingZone_Raw_parkingdata')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureDataLakeStorage_permanentstorage",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "DataLake"
				},
				"annotations": [],
				"type": "AzureBlobFSFile",
				"typeProperties": {
					"compression": {
						"type": "GZip",
						"level": "Optimal"
					},
					"fileName": "[parameters('Datalake_LandingZone_Raw_parkingdata_properties_typeProperties_fileName')]",
					"folderPath": "[parameters('Datalake_LandingZone_Raw_parkingdata_properties_typeProperties_folderPath')]"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureDataLakeStorage_permanentstorage')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Blob_mbdvobist_parkingdata')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "BlobStorage_importstorage",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "Blobs"
				},
				"annotations": [],
				"type": "AzureBlob",
				"typeProperties": {
					"fileName": "[parameters('Blob_mbdvobist_parkingdata_properties_typeProperties_fileName')]",
					"folderPath": "[parameters('Blob_mbdvobist_parkingdata_properties_typeProperties_folderPath')]"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/BlobStorage_importstorage')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Datalake_LandingZone_Raw_parkingdata1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureDataLakeStore_dgdvodltmads",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "DataLake"
				},
				"annotations": [],
				"type": "AzureBlobFSFile",
				"typeProperties": {
					"compression": {
						"type": "GZip",
						"level": "Optimal"
					},
					"fileName": "[parameters('Datalake_LandingZone_Raw_parkingdata1_properties_typeProperties_fileName')]",
					"folderPath": "[parameters('Datalake_LandingZone_Raw_parkingdata1_properties_typeProperties_folderPath')]"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureDataLakeStore_dgdvodltmads')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/DeltaCopy_Blob2DataLake_parking_data')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Copy raw parking data from blob to ADLS",
				"activities": [
					{
						"name": "DeltaCopy_Blob2DataLake_parkingdata",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [
							{
								"name": "Source",
								"value": "parking-data/"
							},
							{
								"name": "Destination",
								"value": "datalake/LandingZone/Raw/parking-data/"
							}
						],
						"typeProperties": {
							"source": {
								"type": "BlobSource",
								"recursive": true
							},
							"sink": {
								"type": "AzureBlobFSSink",
								"copyBehavior": "PreserveHierarchy"
							},
							"enableStaging": false
						},
						"inputs": [
							{
								"referenceName": "Blob_mbdvobist_parkingdata",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "Datalake_LandingZone_Raw_parkingdata",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"folder": {
					"name": "Landing Zone Loads"
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/Blob_mbdvobist_parkingdata')]",
				"[concat(variables('factoryId'), '/datasets/Datalake_LandingZone_Raw_parkingdata')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/DeltaCopy_Blob2DataLake_parking_data1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Copy raw parking data from blob to ADLS",
				"activities": [
					{
						"name": "DeltaCopy_Blob2DataLake_parkingdata",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [
							{
								"name": "Source",
								"value": "parking-data/"
							},
							{
								"name": "Destination",
								"value": "datalake/LandingZone/Raw/parking-data/"
							}
						],
						"typeProperties": {
							"source": {
								"type": "BlobSource",
								"recursive": true
							},
							"sink": {
								"type": "AzureBlobFSSink",
								"copyBehavior": "PreserveHierarchy"
							},
							"enableStaging": false
						},
						"inputs": [
							{
								"referenceName": "Blob_mbdvobist_parkingdata",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "Datalake_LandingZone_Raw_parkingdata1",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"folder": {
					"name": "Landing Zone Loads"
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/Blob_mbdvobist_parkingdata')]",
				"[concat(variables('factoryId'), '/datasets/Datalake_LandingZone_Raw_parkingdata1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/DailyTrigger_TumblingWindow')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Stopped",
				"pipeline": {
					"pipelineReference": {
						"referenceName": "DeltaCopy_Blob2DataLake_parking_data",
						"type": "PipelineReference"
					},
					"parameters": {}
				},
				"type": "TumblingWindowTrigger",
				"typeProperties": {
					"frequency": "Hour",
					"interval": 1,
					"startTime": "2019-04-15T14:44:00Z",
					"delay": "00:00:00",
					"maxConcurrency": 50,
					"retryPolicy": {
						"intervalInSeconds": 30
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/pipelines/DeltaCopy_Blob2DataLake_parking_data')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/DailyTumblingWindow')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Stopped",
				"pipeline": {
					"pipelineReference": {
						"referenceName": "DeltaCopy_Blob2DataLake_parking_data1",
						"type": "PipelineReference"
					},
					"parameters": {}
				},
				"type": "TumblingWindowTrigger",
				"typeProperties": {
					"frequency": "Minute",
					"interval": 15,
					"startTime": "2019-04-14T20:40:00Z",
					"delay": "00:00:00",
					"maxConcurrency": 50,
					"retryPolicy": {
						"intervalInSeconds": 30
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/pipelines/DeltaCopy_Blob2DataLake_parking_data1')]"
			]
		}
	]
}