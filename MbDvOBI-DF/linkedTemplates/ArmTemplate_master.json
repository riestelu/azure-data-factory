{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "MbDvOBI-DF"
		},
		"AzureDataLakeStore_dgdvodltmads_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStore_dgdvodltmads'"
		},
		"BlobStorage_importstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorage_importstorage'"
		},
		"linkedService1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedService1'"
		},
		"AzureDataLakeStore_dgdvodltmads_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dgdvodltmads.dfs.core.windows.net"
		},
		"Blob_mbdvobist_parkingdata_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"Blob_mbdvobist_parkingdata_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "parking-data"
		},
		"Datalake_LandingZone_Raw_parkingdata1_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"Datalake_LandingZone_Raw_parkingdata1_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "datalake/LandingZone/Raw/parking-data"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStore_dgdvodltmads_accountKey": {
						"value": "[parameters('AzureDataLakeStore_dgdvodltmads_accountKey')]"
					},
					"BlobStorage_importstorage_connectionString": {
						"value": "[parameters('BlobStorage_importstorage_connectionString')]"
					},
					"linkedService1_connectionString": {
						"value": "[parameters('linkedService1_connectionString')]"
					},
					"AzureDataLakeStore_dgdvodltmads_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStore_dgdvodltmads_properties_typeProperties_url')]"
					},
					"Blob_mbdvobist_parkingdata_properties_typeProperties_fileName": {
						"value": "[parameters('Blob_mbdvobist_parkingdata_properties_typeProperties_fileName')]"
					},
					"Blob_mbdvobist_parkingdata_properties_typeProperties_folderPath": {
						"value": "[parameters('Blob_mbdvobist_parkingdata_properties_typeProperties_folderPath')]"
					},
					"Datalake_LandingZone_Raw_parkingdata1_properties_typeProperties_fileName": {
						"value": "[parameters('Datalake_LandingZone_Raw_parkingdata1_properties_typeProperties_fileName')]"
					},
					"Datalake_LandingZone_Raw_parkingdata1_properties_typeProperties_folderPath": {
						"value": "[parameters('Datalake_LandingZone_Raw_parkingdata1_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}